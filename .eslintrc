{
    "extends": "standard",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "no-console": 0,
        "semi": 2,
        "no-new": 0,
        "no-self-compare": 0
    }
}
